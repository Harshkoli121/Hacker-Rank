#include <iostream>
#include <cstdio>
using namespace std;
int max_of_four(int a,int b,int c,int d){
    int gre;
    if(a>b&&a>c&&a>d){
        gre=a;
    }else if(b>c&&b>d){
        gre=b;
    }else if(c>d){
        gre=c;
    }else {
        gre=d;
    }
    return gre;
}
int main() {
    int a,b,c,d;
    cin>>a;
    cin>>b;
    cin>>c;
    cin>>d;
    int ans = max_of_four(a, b, c, d);
    cout<<ans;
    return 0;
}
